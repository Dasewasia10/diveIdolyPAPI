      "bgName": "stage2"
    },
    {
      "type": "dialogue",
      "speakerCode": "koh",
      "speakerName": "{user}",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-kohei.png",
      "voiceUrl": "https://api.diveidolypapi.my.id/mainStoryVoice/sud_vo_adv_main_01_06_07/sud_vo_adv_main_01_06_07-koh007.m4a",
      "text": "（まるで存在を隠したがっているようだし）",
