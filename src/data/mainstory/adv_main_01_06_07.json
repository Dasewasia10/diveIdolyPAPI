      "startTime": 133.8635887544604,
      "sfxList": []
    },
    {
      "type": "background",
      "src": "https://api.diveidolypapi.my.id/storyBackground/env_adv_2d_hoshistore-01-noon.webp",
      "bgName": "stage2"
    },
    {
