      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-rei.png",
      "voiceUrl": "https://api.diveidolypapi.my.id/mainStoryVoice/sud_vo_adv_main_01_06_06/sud_vo_adv_main_01_06_06-rei003.m4a",
      "text": "今のはしっかり足が\n上がってなかったからですね",
      "startTime": 61.18810756727871,
      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "skr",
      "speakerName": "川咲さくら",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-sakura.png",
