      "bgName": "stage01"
    },
    {
      "type": "dialogue",
      "speakerCode": "ski",
      "speakerName": "白石沙季",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-saki.png",
      "voiceUrl": "https://api.diveidolypapi.my.id/mainStoryVoice/sud_vo_adv_main_01_09_04/sud_vo_adv_main_01_09_04-ski012.m4a",
      "text": "でも、心配したから送るんじゃなくて……\nもっと、違う形で送ってみます",
      "startTime": 95.3441723068556,
      "sfxList": []
    },
    {
      "type": "background",
      "src": "https://api.diveidolypapi.my.id/storyBackground/env_adv_2d_designcorridor-00-noon.webp",
      "bgName": "stage01"
