      "startTime": 86.6878683924675,
      "sfxList": []
    },
    {
      "type": "background",
      "src": "https://api.diveidolypapi.my.id/storyBackground/env_adv_2d_designcorridor-00-noon.webp",
      "bgName": "stage01"
    },
    {
      "type": "dialogue",
      "speakerCode": "ski",
