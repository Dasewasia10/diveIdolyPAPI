      "startTime": 112.4999961692,
      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "ngs",
      "speakerName": "伊吹渚",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-nagisa.png",
