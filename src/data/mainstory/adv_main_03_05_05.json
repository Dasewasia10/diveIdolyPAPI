      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "ngs",
      "speakerName": "伊吹渚",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-nagisa.png",
      "voiceUrl": "https://api.diveidolypapi.my.id/mainStoryVoice/sud_vo_adv_main_03_05_05/sud_vo_adv_main_03_05_05-ngs016.m4a",
