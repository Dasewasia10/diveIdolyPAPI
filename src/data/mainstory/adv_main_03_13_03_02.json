      "voiceUrl": "https://api.diveidolypapi.my.id/mainStoryVoice/sud_vo_adv_main_03_13_03_02/sud_vo_adv_main_03_13_03-lim004.m4a",
      "text": "……あ？\n恥を晒して生きろってのか？　お前らみたいに？",
      "startTime": 128.3666655633,
      "sfxList": []
    },
    {
      "type": "background",
      "src": "https://api.diveidolypapi.my.id/storyBackground/env_adv_2d_big4challengestage-00-noon.webp",
      "bgName": "big4challengestage-00-noon"
    },
    {
      "type": "dialogue",
      "speakerCode": "ktn",
      "speakerName": "長瀬琴乃",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-kotono.png",
