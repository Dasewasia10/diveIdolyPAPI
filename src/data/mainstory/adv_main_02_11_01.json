      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-kohei.png",
      "voiceUrl": "https://api.diveidolypapi.my.id/mainStoryVoice/sud_vo_adv_main_02_11_01/sud_vo_adv_main_02_11_01-koh001.m4a",
      "text": "天動さん……",
      "startTime": 13.4666666667,
      "sfxList": []
    },
    {
      "type": "background",
      "src": "https://api.diveidolypapi.my.id/storyBackground/env_adv_2d_festivalseat-01-noon.webp",
