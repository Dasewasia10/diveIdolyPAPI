    {
      "type": "dialogue",
      "speakerCode": "kkr",
      "speakerName": "赤崎こころ",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-kokoro.png",
      "voiceUrl": null,
      "text": "確かに告知はしてませんけど……大人気アイドルの",
