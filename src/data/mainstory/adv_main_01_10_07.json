    },
    {
      "type": "dialogue",
      "speakerCode": "ktn",
      "speakerName": "長瀬琴乃",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-kotono.png",
      "voiceUrl": "https://api.diveidolypapi.my.id/mainStoryVoice/sud_vo_adv_main_01_10_07/sud_vo_adv_main_01_10_02-ktn017.m4a",
      "text": "…………ですよね",
      "startTime": 161.13333333333333,
      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "koh",
      "speakerName": "{user}",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-kohei.png",
