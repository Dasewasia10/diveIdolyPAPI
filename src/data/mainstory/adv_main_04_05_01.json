      "speakerName": "{user}",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-kohei.png",
      "voiceUrl": "https://api.diveidolypapi.my.id/mainStoryVoice/sud_vo_adv_main_04_05_01/sud_vo_adv_main_04_05_01-koh002.m4a",
      "text": "そのことなんだが……",
      "startTime": 88.68412931,
      "sfxList": []
    },
    {
      "type": "background",
      "src": "https://api.diveidolypapi.my.id/storyBackground/env_adv_2d_hospital-00-night.webp",
      "bgName": "hospital-00-night"
