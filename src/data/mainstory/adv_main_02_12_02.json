      "bgName": "stage01"
    },
    {
      "type": "dialogue",
      "speakerCode": "ski",
      "speakerName": "白石沙季",
      "iconUrl": "https://apiip.dasewasia.my.id/iconCharacter/chara-saki.png",
      "voiceUrl": "https://apiip.dasewasia.my.id/mainStoryVoice/sud_vo_adv_main_02_12_02/sud_vo_adv_main_02_12_02-ski001.m4a",
      "text": "お願いします、{user}さん……\n私達に、言葉をかけてくれませんか",
      "startTime": 110.2666666667,
      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "koh",
      "speakerName": "{user}",
      "iconUrl": "https://apiip.dasewasia.my.id/iconCharacter/chara-kohei.png",
