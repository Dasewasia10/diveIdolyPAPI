      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "koh",
      "speakerName": "{user}",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-kohei.png",
      "voiceUrl": "https://api.diveidolypapi.my.id/mainStoryVoice/sud_vo_adv_main_01_07_07/sud_vo_adv_main_01_07_07-koh008.m4a",
