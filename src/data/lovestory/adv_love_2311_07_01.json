      "text": "そりゃそうだよ、だって……",
      "startTime": 63.666666484,
      "sfxList": []
    },
    {
      "type": "choice_selection",
      "choices": [
        {
          "text": "将来への不安を訴える",
          "route": [
            {
              "type": "bgm",
              "action": "stop"
            },
            {
              "type": "dialogue",
              "speakerCode": "koh",
