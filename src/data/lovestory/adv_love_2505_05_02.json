    {
      "type": "dialogue",
      "speakerCode": null,
      "speakerName": "",
      "iconUrl": null,
      "voiceUrl": null,
      "text": "「ずっとルイのそばにいる」\nそう心に決めて、ルイに伝えた",
      "startTime": 0.8333333333,
      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "rui",
      "speakerName": "ルイ",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-rui.png",
