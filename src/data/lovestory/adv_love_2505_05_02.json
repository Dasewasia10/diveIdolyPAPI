    },
    {
      "type": "dialogue",
      "speakerCode": null,
      "speakerName": "",
      "iconUrl": null,
      "voiceUrl": null,
      "text": "それもつかの間\nすみれの魔法が押し切り\nルイを飲み込んでいった",
      "startTime": 40.1665514625,
      "sfxList": []
    },
    {
      "type": "background",
      "src": "https://api.diveidolypapi.my.id/storyBackground/env_adv_2d_outsidetv-00-evening.webp",
