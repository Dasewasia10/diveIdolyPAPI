      "voiceUrl": "https://api.diveidolypapi.my.id/lovestoryVoice/sud_vo_adv_love_2505_05_02/sud_vo_adv_love_2505_05_01-smr009.wav",
      "text": "はああああああああああああ！！",
      "startTime": 34.3332364397,
      "sfxList": []
    },
    {
      "type": "background",
      "src": "https://api.diveidolypapi.my.id/storyBackground/env_adv_2d_outsidetv-00-evening.webp",
