    },
    {
      "type": "dialogue",
      "speakerCode": null,
      "speakerName": "",
      "iconUrl": null,
      "voiceUrl": null,
      "text": "激しい閃光が周囲を包む",
      "startTime": 37.2665494279,
      "sfxList": []
    },
    {
      "type": "bgm",
      "action": "stop"
    },
    {
      "type": "bgm",
