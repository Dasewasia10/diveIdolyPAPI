    },
    {
      "type": "dialogue",
      "speakerCode": null,
      "speakerName": "",
      "iconUrl": null,
      "voiceUrl": null,
      "text": "消えゆくルイと、視線が交わる",
      "startTime": 45.1665372209,
      "sfxList": []
    },
    {
      "type": "background",
      "src": "https://api.diveidolypapi.my.id/storyBackground/env_adv_2d_outsidetv-00-evening.webp",
