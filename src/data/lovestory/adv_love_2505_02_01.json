    {
      "type": "bgm",
      "action": "stop"
    },
    {
      "type": "dialogue",
      "speakerCode": null,
      "speakerName": "",
      "iconUrl": null,
      "voiceUrl": null,
      "text": "すみれが誇らしげに胸を張っている"
    },
    {
      "type": "choice_selection",
      "choices": [
        {
          "text": "すみれは凄いんだね",
          "route": [
            {
              "type": "bgm",
              "action": "play",
