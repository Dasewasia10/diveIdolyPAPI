    },
    {
      "type": "dialogue",
      "speakerCode": null,
      "speakerName": "",
      "iconUrl": null,
      "voiceUrl": null,
      "text": "渚はどこに行ったのだろう"
    },
    {
      "type": "background",
      "src": "https://api.diveidolypapi.my.id/storyBackground/env_adv_2d_tokyopark-00-night.webp",
