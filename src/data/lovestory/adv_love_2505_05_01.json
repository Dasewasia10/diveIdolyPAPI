      "startTime": 108.6666327037
    },
    {
      "type": "dialogue",
      "speakerCode": null,
      "speakerName": "",
      "iconUrl": null,
      "voiceUrl": null,
      "text": "ルイが顔を近づけてきた",
      "startTime": 109.5333014049,
      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "rui",
