    },
    {
      "type": "dialogue",
      "speakerCode": null,
      "speakerName": "",
      "iconUrl": null,
      "voiceUrl": null,
      "text": "生命エネルギーを得たルイの力は\n凄まじいものだった",
      "startTime": 39.9666601698,
      "sfxList": []
    },
    {
      "type": "background",
