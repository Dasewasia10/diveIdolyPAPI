    },
    {
      "type": "dialogue",
      "speakerCode": "chs",
      "speakerName": "チサ",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-chisa.png",
      "voiceUrl": "https://api.diveidolypapi.my.id/lovestoryVoice/sud_vo_adv_love_2505_01_01/sud_vo_adv_love_2505_01_01-chs013.wav",
      "text": "ひゃあっ！？"
    },
    {
      "type": "background",
      "src": "https://api.diveidolypapi.my.id/storyBackground/env_adv_2d_garden-00-night.webp",
      "bgName": "garden-00-night"
    },
    {
