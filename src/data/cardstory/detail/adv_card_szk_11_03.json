      },
      "text": "この演出は、私もいいアイデアだと思った……",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_szk_11_03/sud_vo_adv_card_szk_11_03-szk005.m4a"
