      "speaker": {
        "isPlayer": false,
        "characterId": "mhk",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-miho.png",
        "name": "miho"
      },
      "text": "この近辺で、扱っているお店を探して……って",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_mhk_52_02/sud_vo_adv_card_mhk_52_02-mhk004.m4a"
    },
