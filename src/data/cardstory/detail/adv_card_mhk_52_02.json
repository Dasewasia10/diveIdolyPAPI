      "speaker": {
        "isPlayer": false,
        "characterId": "mhk",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-miho.png",
        "name": "miho"
      },
      "text": "いきなり山梨くんだりまで連れて来られたので",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_mhk_52_02/sud_vo_adv_card_mhk_52_02-mhk006.m4a"
    },
