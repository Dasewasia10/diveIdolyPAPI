      "id": "msg_17",
      "speaker": {
        "isPlayer": false,
        "characterId": "mhk",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-miho.png",
        "name": "miho"
      },
      "text": "なんですか。人を飲んだくれのように見て",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_mhk_52_02/sud_vo_adv_card_mhk_52_02-mhk005.m4a"
    },
