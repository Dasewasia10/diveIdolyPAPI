      "id": "msg_16",
      "speaker": {
        "isPlayer": false,
        "characterId": "ktn",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-kotono.png",
        "name": "長瀬琴乃"
      },
      "text": "全体的な演技は、練習のたびに",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_ktn_07_03/sud_vo_adv_card_ktn_07_03-ktn005.m4a"
    },
