      },
      "text": "より美味しく感じるだろうな",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_suz_07_03/sud_vo_adv_card_suz_07_03-koh013.m4a"
