      "speaker": {
        "isPlayer": false,
        "characterId": "rio",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-rio.png",
        "name": "神崎莉央"
      },
      "text": "それなら電車を乗り継いで行きましょう",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_rio_10_01_short/sud_vo_adv_card_rio_10_01-koh002.m4a"
    },
