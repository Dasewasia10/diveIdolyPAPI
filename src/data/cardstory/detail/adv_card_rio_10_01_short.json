      "speaker": {
        "isPlayer": false,
        "characterId": "rio",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-rio.png",
        "name": "神崎莉央"
      },
      "text": "ほら、急ぎなさい、走るわよ！",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_rio_10_01_short/sud_vo_adv_card_rio_10_01-rio003.m4a"
    },
