      "speaker": {
        "isPlayer": false,
        "characterId": "ngs",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-nagisa.png",
        "name": "伊吹渚"
      },
      "text": "全然そんなことないよ。えへへ、お母さんが料理作り",
      "isChoice": false,
