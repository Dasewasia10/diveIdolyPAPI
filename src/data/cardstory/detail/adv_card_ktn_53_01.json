      "speaker": {
        "isPlayer": false,
        "characterId": "ktn",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-kotono.png",
        "name": "長瀬琴乃"
      },
      "text": "（でも届いた連絡は……どこかで予想していたもので……）",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_ktn_53_01/sud_vo_adv_card_ktn_53_01-ktn008.m4a"
    },
