      "speaker": {
        "isPlayer": false,
        "characterId": "kkr",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-kokoro.png",
        "name": "赤崎こころ"
      },
      "text": "自らハードルを上げていくスタイル！",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_kkr_51_01/sud_vo_adv_card_kkr_51_01-ai006.m4a"
