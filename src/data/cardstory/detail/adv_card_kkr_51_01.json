      "speaker": {
        "isPlayer": false,
        "characterId": "kkr",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-kokoro.png",
        "name": "赤崎こころ"
      },
      "text": "企画意図的にもそっちの方が良くない？",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_kkr_51_01/sud_vo_adv_card_kkr_51_01-kkr008.m4a"
    },
