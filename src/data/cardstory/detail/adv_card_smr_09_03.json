      "speaker": {
        "isPlayer": false,
        "characterId": "smr",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-sumire.png",
        "name": "奥山すみれ"
      },
      "text": "……いえ、なんでもありません",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_smr_09_03/sud_vo_adv_card_smr_09_03-smr003.m4a"
    },
