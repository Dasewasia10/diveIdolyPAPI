      "speaker": {
        "isPlayer": true,
        "characterId": "koh",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-makino.png",
        "name": "{user}"
      },
      "text": "疲れただろう。少しゆっくりしてきてくれ",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_aoi_11_03/sud_vo_adv_card_aoi_11_03-koh012.m4a"
