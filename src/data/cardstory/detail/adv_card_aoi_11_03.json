      "speaker": {
        "isPlayer": false,
        "characterId": "aoi",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-aoi.png",
        "name": "井川葵"
      },
      "text": "そのホテルは朝食が美味しくて有名な所でね",
