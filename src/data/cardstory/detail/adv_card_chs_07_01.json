      "speaker": {
        "isPlayer": false,
        "characterId": "chs",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-chisa.png",
        "name": "白石千紗"
      },
      "text": "家族みんなで流星群を見て帰るのが",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_chs_07_01/sud_vo_adv_card_chs_07_01-chs006.m4a"
    },
