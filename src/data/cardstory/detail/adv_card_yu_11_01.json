      "id": "msg_21",
      "speaker": {
        "isPlayer": false,
        "characterId": "yu",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-yu.png",
        "name": "鈴村優"
      },
      "text": "うちにもし恋の話、持ってきてくれて",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_yu_11_01/sud_vo_adv_card_yu_11_01-yu008.m4a"
    },
