      "speaker": {
        "isPlayer": false,
        "characterId": "yu",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-yu.png",
        "name": "鈴村優"
      },
      "text": "きっとすぐアンケート用紙真っ黒になるからな",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_yu_11_01/sud_vo_adv_card_yu_11_01-yu011.m4a"
