      "speaker": {
        "isPlayer": false,
        "characterId": "aoi",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-aoi.png",
        "name": "井川葵"
      },
      "text": "即興でも出来る振り付けを入れてある",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_aoi_12_03/sud_vo_adv_card_aoi_12_03-aoi011.m4a"
    },
