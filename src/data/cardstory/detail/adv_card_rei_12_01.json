      "speaker": {
        "isPlayer": false,
        "characterId": "rei",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-rei.png",
        "name": "一ノ瀬怜"
      },
      "text": "だから、夢じゃないって言ってるじゃないですか",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_rei_12_01/sud_vo_adv_card_rei_12_01-rei008.m4a"
