      "id": "msg_17",
      "speaker": {
        "isPlayer": true,
        "characterId": "koh",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-makino.png",
        "name": "{user}"
      },
      "text": "確か、千紗が夢の中で着ていた制服を再現したとか……",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_rei_12_01/sud_vo_adv_card_rei_12_01-koh004.m4a"
    },
