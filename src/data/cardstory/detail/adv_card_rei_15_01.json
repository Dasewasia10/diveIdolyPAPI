      "speaker": {
        "isPlayer": false,
        "characterId": "rei",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-rei.png",
        "name": "一ノ瀬怜"
      },
      "text": "何を隠しているんですか！？",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_rei_15_01/sud_vo_adv_card_rei_15_01-rei007.m4a"
