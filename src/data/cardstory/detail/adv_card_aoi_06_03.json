      "id": "msg_16",
      "speaker": {
        "isPlayer": true,
        "characterId": "koh",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-makino.png",
        "name": "{user}"
      },
      "text": "そのまま、ドラマの制作側に伝えてみるのはどうだろう",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_aoi_06_03/sud_vo_adv_card_aoi_06_03-koh001.m4a"
    },
