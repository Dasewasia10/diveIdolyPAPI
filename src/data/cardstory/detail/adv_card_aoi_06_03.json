      "speaker": {
        "isPlayer": false,
        "characterId": "aoi",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-aoi.png",
        "name": "井川葵"
      },
      "text": "どうしようか",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_aoi_06_03/sud_vo_adv_card_aoi_06_03-aoi007.m4a"
    },
    {
