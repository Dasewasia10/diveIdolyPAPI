      "id": "msg_51",
      "speaker": {
        "isPlayer": false,
        "characterId": "system",
        "icon": null,
        "name": "System"
      },
      "text": "――撮影当日",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": null
