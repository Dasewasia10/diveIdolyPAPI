      "id": "msg_20",
      "speaker": {
        "isPlayer": false,
        "characterId": "kor",
        "icon": null,
        "name": "fran"
      },
      "text": "本業を疎かにするなとは言わないんですか？",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_kor_01_03/sud_vo_adv_card_kor_01_03-kor006.m4a"
    },
