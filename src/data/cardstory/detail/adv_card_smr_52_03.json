      "speaker": {
        "isPlayer": false,
        "characterId": "smr",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-sumire.png",
        "name": "奥山すみれ"
      },
      "text": "雪の声が導く通りに手を動かすんです。そうすれば",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_smr_52_03/sud_vo_adv_card_smr_52_03-smr005.m4a"
    },
