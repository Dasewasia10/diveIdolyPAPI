      "id": "msg_49",
      "speaker": {
        "isPlayer": false,
        "characterId": "ktn",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-kotono.png",
        "name": "長瀬琴乃"
      },
      "text": "五人揃って歌ったら……いつの間にか晴れてたの",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_ktn_09_03/sud_vo_adv_card_ktn_09_03-ktn019.m4a"
