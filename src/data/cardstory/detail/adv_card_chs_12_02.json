      "speaker": {
        "isPlayer": false,
        "characterId": "chs",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-chisa.png",
        "name": "白石千紗"
      },
      "text": "協力してくれたみなさんのおかげです",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_chs_12_02/sud_vo_adv_card_chs_12_02-chs001.m4a"
