      "speaker": {
        "isPlayer": false,
        "characterId": "rei",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-rei.png",
        "name": "一ノ瀬怜"
      },
      "text": "誕生日企画はもう始まってるのよ？",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_rei_51_03/sud_vo_adv_card_rei_51_03-rei002.m4a"
