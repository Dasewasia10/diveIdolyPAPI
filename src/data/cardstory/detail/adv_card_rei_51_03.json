      "speaker": {
        "isPlayer": false,
        "characterId": "suz",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-suzu.png",
        "name": "成宮すず"
      },
      "text": "私、二度寝がしたいですわ",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_rei_51_03/sud_vo_adv_card_rei_51_03-suz002.m4a"
