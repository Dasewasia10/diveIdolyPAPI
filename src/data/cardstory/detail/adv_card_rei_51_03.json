      "speaker": {
        "isPlayer": false,
        "characterId": "rei",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-rei.png",
        "name": "一ノ瀬怜"
      },
      "text": "あ、そろそろスタッフさんのこと迎えに行こうかしら",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_rei_51_03/sud_vo_adv_card_rei_51_03-rei006.m4a"
    },
    {
      "id": "msg_23",
      "speaker": {
        "isPlayer": false,
        "characterId": "mei",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-mei.png",
        "name": "早坂芽衣"
      },
      "text": "怜ちゃん待ってぇ……さすがにもうちょっと",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_rei_51_03/sud_vo_adv_card_rei_51_03-mei004.m4a"
    },
