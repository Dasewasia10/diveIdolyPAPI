        "isPlayer": true,
        "characterId": "koh",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-makino.png",
        "name": "{user}"
      },
      "text": "そういえば、今日は現場でスタッフさんと",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_rui_06_02/sud_vo_adv_card_rui_06_02-koh004.m4a"
