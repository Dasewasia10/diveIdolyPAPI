    {
      "id": "msg_82",
      "speaker": {
        "isPlayer": false,
        "characterId": "ski",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-saki.png",
        "name": "白石沙季"
      },
      "text": "私の魅力が眠っているのか分かりませんから！",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_ski_04_01/sud_vo_adv_card_ski_04_01-ski027.m4a"
