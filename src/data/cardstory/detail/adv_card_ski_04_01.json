      "id": "msg_81",
      "speaker": {
        "isPlayer": false,
        "characterId": "ski",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-saki.png",
        "name": "白石沙季"
      },
      "text": "大丈夫です！　どんなところに",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_ski_04_01/sud_vo_adv_card_ski_04_01-koh016.m4a"
