      "speaker": {
        "isPlayer": false,
        "characterId": "kkr",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-kokoro.png",
        "name": "赤崎こころ"
      },
      "text": "まだ計画中なので、誰にとかは秘密ですが。それよりも",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_kkr_14_02/sud_vo_adv_card_kkr_14_02-kkr011.m4a"
