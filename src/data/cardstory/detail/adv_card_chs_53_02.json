      "id": "msg_14",
      "speaker": {
        "isPlayer": false,
        "characterId": "szk",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-shizuku.png",
        "name": "兵藤雫"
      },
      "text": "うん。あの時は千紗ちゃんが、みんなとお弁当を",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_chs_53_02/sud_vo_adv_card_chs_53_02-chs003.m4a"
    },
