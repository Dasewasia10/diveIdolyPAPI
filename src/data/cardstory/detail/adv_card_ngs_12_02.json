      "speaker": {
        "isPlayer": false,
        "characterId": "ngs",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-nagisa.png",
        "name": "伊吹渚"
      },
      "text": "次は脱出ゲームです。今度は一緒に楽しみましょう",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_ngs_12_02/sud_vo_adv_card_ngs_12_02-ngs005.m4a"
    },
