      "speaker": {
        "isPlayer": false,
        "characterId": "ngs",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-nagisa.png",
        "name": "伊吹渚"
      },
      "text": "ふふ、ご主人様も乗ってきましたね",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_ngs_12_02/sud_vo_adv_card_ngs_12_02-koh004.m4a"
