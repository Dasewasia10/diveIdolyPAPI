      "speaker": {
        "isPlayer": true,
        "characterId": "koh",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-makino.png",
        "name": "{user}"
      },
      "text": "結果的に来なくて良かった。あれは刺激が強過ぎる……",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_ngs_12_02/sud_vo_adv_card_ngs_12_02-koh003.m4a"
    },
