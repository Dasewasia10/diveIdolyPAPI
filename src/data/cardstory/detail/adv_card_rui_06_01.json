      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_rui_06_01/sud_vo_adv_card_rui_06_01-rui002.m4a"
