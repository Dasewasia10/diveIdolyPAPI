      "speaker": {
        "isPlayer": false,
        "characterId": "skr",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-sakura.png",
        "name": "川咲さくら"
      },
      "text": "明るいだなんて……えへへへ～",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_skr_09_01/sud_vo_adv_card_skr_09_01-skr009.m4a"
    },
    {
      "id": "msg_25",
      "speaker": {
        "isPlayer": true,
        "characterId": "koh",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-makino.png",
        "name": "{user}"
      },
      "text": "今までにない新たな挑戦になると思うが",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_skr_09_01/sud_vo_adv_card_skr_09_01-koh005.m4a"
