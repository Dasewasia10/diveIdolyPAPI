      "id": "msg_10",
      "speaker": {
        "isPlayer": true,
        "characterId": "koh",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-makino.png",
        "name": "{user}"
      },
      "text": "あぁ、応援しているし、精一杯支えるよ",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_rui_10_02/sud_vo_adv_card_rui_10_02-rui003.m4a"
    },
