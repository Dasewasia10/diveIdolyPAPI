      "speaker": {
        "isPlayer": false,
        "characterId": "rui",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-rui.png",
        "name": "天動瑠依"
      },
      "text": "最終調整をしながら、生放送も観てくださるそうです",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_rui_10_02/sud_vo_adv_card_rui_10_02-rui011.m4a"
    },
