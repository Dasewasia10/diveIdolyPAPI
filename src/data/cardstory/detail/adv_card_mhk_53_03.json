      "id": "msg_36",
      "speaker": {
        "isPlayer": false,
        "characterId": "kor",
        "icon": null,
        "name": "fran"
      },
      "text": "三人……そうよ、私達二人だけっておかしくない？",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mhk_53_03/sud_vo_adv_card_mhk_53_03-kor008.m4a"
    },
