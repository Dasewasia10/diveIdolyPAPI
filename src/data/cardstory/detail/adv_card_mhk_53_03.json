      "id": "msg_37",
      "speaker": {
        "isPlayer": false,
        "characterId": "kor",
        "icon": null,
        "name": "fran"
      },
      "text": "いくらバズったとしても、私達だけ苦労するって変よ",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mhk_53_03/sud_vo_adv_card_mhk_53_03-kor008.m4a"
    },
