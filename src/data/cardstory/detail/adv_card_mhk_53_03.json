      "id": "msg_15",
      "speaker": {
        "isPlayer": false,
        "characterId": "kor",
        "icon": null,
        "name": "fran"
      },
      "text": "てか誕生日って誰の！　と、とにかく確認しないと",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mhk_53_03/sud_vo_adv_card_mhk_53_03-kor004.m4a"
    },
