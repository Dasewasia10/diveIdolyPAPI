      "speaker": {
        "isPlayer": false,
        "characterId": "mhk",
        "icon": null,
        "name": "miho"
      },
      "text": "とっても可愛いわ。ぐっにゃいって言うのね、可愛い",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mhk_53_03/sud_vo_adv_card_mhk_53_03-mhk004.m4a"
