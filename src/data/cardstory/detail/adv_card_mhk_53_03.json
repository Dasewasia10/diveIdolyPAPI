      "speaker": {
        "isPlayer": false,
        "characterId": "kor",
        "icon": null,
        "name": "fran"
      },
      "text": "動画に出なきゃいけないのよ！　ありえないんだけど！",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mhk_53_03/sud_vo_adv_card_mhk_53_03-kor001.m4a"
