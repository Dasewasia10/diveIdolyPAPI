      "id": "msg_38",
      "speaker": {
        "isPlayer": false,
        "characterId": "ski",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-saki.png",
        "name": "白石沙季"
      },
      "text": "はぁ、やっぱり寝てる。ほら、毛布持ってきましたから",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_ski_53_03/sud_vo_adv_card_ski_53_03-hrk010.m4a"
