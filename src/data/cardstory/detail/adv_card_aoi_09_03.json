      "speaker": {
        "isPlayer": false,
        "characterId": "aoi",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-aoi.png",
        "name": "井川葵"
      },
      "text": "当然だよ。ただでさえ宿題で三週間も潰したんだ",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_aoi_09_03/sud_vo_adv_card_aoi_09_03-aoi002.m4a"
    },
