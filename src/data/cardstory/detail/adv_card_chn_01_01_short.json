      "id": "msg_11",
      "speaker": {
        "isPlayer": false,
        "characterId": "chn",
        "icon": null,
        "name": "チノ"
      },
      "text": "お姉ちゃんになりたがります。見境がないです",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_chn_01_01_short/sud_vo_adv_card_chn_01_01-chn002.m4a"
