      "speaker": {
        "isPlayer": false,
        "characterId": "mhk",
        "icon": null,
        "name": "miho"
      },
      "text": "車のメーカーの人達も見に来ているのに",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mhk_08_03/sud_vo_adv_card_mhk_08_03-mhk011.m4a"
    },
