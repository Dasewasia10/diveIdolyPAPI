      "id": "msg_32",
      "speaker": {
        "isPlayer": true,
        "characterId": "koh",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-makino.png",
        "name": "{user}"
      },
      "text": "え！？　そんなエンジンを吹かして、一体どうする",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mhk_08_03/sud_vo_adv_card_mhk_08_03-koh007.m4a"
    },
