      "id": "msg_39",
      "speaker": {
        "isPlayer": false,
        "characterId": "mhk",
        "icon": null,
        "name": "miho"
      },
      "text": "なかなか迫力のあるドリフトだったでしょう？",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mhk_08_03/sud_vo_adv_card_mhk_08_03-mhk010.m4a"
    },
