      "speaker": {
        "isPlayer": false,
        "characterId": "mhk",
        "icon": null,
        "name": "miho"
      },
      "text": "おそらく貴方の気のせいでしょう",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mhk_09_01/sud_vo_adv_card_mhk_09_01-mhk014.m4a"
