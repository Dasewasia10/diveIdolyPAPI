      "id": "msg_12",
      "speaker": {
        "isPlayer": false,
        "characterId": "ktn",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-kotono.png",
        "name": "長瀬琴乃"
      },
      "text": "ほ、本当に、あなた、が～、この組織にいる",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_ktn_07_02/sud_vo_adv_card_ktn_07_02-ktn006.m4a"
    },
