      "speaker": {
        "isPlayer": false,
        "characterId": "yu",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-yu.png",
        "name": "鈴村優"
      },
      "text": "かけまして、月のテンペストとときます、みたいな",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_ski_53_02/sud_vo_adv_card_ski_53_02-yu006.m4a"
