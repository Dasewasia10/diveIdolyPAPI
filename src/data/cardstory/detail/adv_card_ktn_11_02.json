      "id": "msg_14",
      "speaker": {
        "isPlayer": false,
        "characterId": "ktn",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-kotono.png",
        "name": "長瀬琴乃"
      },
      "text": "使うんですけど、相手がいないと練習出来なくて",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_ktn_11_02/sud_vo_adv_card_ktn_11_02-ktn005.m4a"
