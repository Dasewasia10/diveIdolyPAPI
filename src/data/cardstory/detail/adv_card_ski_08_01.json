    {
      "id": "msg_83",
      "speaker": {
        "isPlayer": true,
        "characterId": "koh",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-makino.png",
        "name": "{user}"
      },
      "text": "ごめんごめん、でもディレクターさんも沙季の解説を",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_ski_08_01/sud_vo_adv_card_ski_08_01-koh015.m4a"
