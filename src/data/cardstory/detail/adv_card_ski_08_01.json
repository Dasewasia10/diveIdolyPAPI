    {
      "id": "msg_81",
      "speaker": {
        "isPlayer": false,
        "characterId": "ski",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-saki.png",
        "name": "白石沙季"
      },
      "text": "そ、そこは狙って笑わせたんじゃありません！",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_ski_08_01/sud_vo_adv_card_ski_08_01-ski028.m4a"
    },
    {
      "id": "msg_82",
      "speaker": {
        "isPlayer": false,
        "characterId": "ski",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-saki.png",
        "name": "白石沙季"
      },
      "text": "もう、本当に恥ずかしかったんですから",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_ski_08_01/sud_vo_adv_card_ski_08_01-ski028.m4a"
