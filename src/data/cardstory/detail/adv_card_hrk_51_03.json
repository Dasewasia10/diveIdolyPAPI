      "id": "msg_62",
      "speaker": {
        "isPlayer": false,
        "characterId": "mhk",
        "icon": null,
        "name": "miho"
      },
      "text": "どこをどう切り取ったらそう見えるのかしら",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_hrk_51_03/sud_vo_adv_card_hrk_51_03-mhk008.m4a"
