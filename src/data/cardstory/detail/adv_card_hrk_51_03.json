      "id": "msg_79",
      "speaker": {
        "isPlayer": false,
        "characterId": "hrk",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-haruko.png",
        "name": "佐伯遙子"
      },
      "text": "ふふふ、今からわくわくしてきたわ",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_hrk_51_03/sud_vo_adv_card_hrk_51_03-hrk021.m4a"
    },
    {
