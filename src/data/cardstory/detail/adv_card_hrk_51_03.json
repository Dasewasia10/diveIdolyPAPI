    {
      "id": "msg_44",
      "speaker": {
        "isPlayer": false,
        "characterId": "rio",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-rio.png",
        "name": "神崎莉央"
      },
      "text": "遙子の誕生日企画なのに、なんだか",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_hrk_51_03/sud_vo_adv_card_hrk_51_03-rio008.m4a"
