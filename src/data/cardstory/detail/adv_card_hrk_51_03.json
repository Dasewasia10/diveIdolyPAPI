    {
      "id": "msg_43",
      "speaker": {
        "isPlayer": false,
        "characterId": "mhk",
        "icon": null,
        "name": "miho"
      },
      "text": "変な所に連れてかれるのかと思ったけど",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_hrk_51_03/sud_vo_adv_card_hrk_51_03-mhk005.m4a"
