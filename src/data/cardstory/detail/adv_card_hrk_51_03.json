      "speaker": {
        "isPlayer": false,
        "characterId": "hrk",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-haruko.png",
        "name": "佐伯遙子"
      },
      "text": "ねえmihoさん、今度二人でユニット組んでみる？",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_hrk_51_03/sud_vo_adv_card_hrk_51_03-mhk008.m4a"
