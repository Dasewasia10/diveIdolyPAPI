      "speaker": {
        "isPlayer": false,
        "characterId": "unknown",
        "icon": null,
        "name": "莉央の母"
      },
      "text": "日頃、莉央がお世話になっているお礼を言わないと",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_rio_06_02/sud_vo_adv_card_rio_06_02-riomom003.m4a"
