      "speaker": {
        "isPlayer": false,
        "characterId": "unknown",
        "icon": null,
        "name": "莉央の母"
      },
      "text": "連れて来るなんて……！　お母さん、嬉しいわ",
      "isChoice": false,
      "stamp": null,
      "image": null,
