        "isPlayer": false,
        "characterId": "aoi",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-aoi.png",
        "name": "井川葵"
      },
      "text": "鳴かぬなら、鳴くまで斬ろうホトトギス",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_aoi_07_02/sud_vo_adv_card_aoi_07_02-aoi010.m4a"
