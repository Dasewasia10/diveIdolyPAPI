      "id": "msg_27",
      "speaker": {
        "isPlayer": false,
        "characterId": "rei",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-rei.png",
        "name": "一ノ瀬怜"
      },
      "text": "私一人だったら無人島の生活に耐えられなかった……",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_rei_10_03/sud_vo_adv_card_rei_10_03-rei010.m4a"
    },
