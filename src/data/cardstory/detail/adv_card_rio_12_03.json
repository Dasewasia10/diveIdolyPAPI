    {
      "id": "msg_48",
      "speaker": {
        "isPlayer": false,
        "characterId": "rio",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-rio.png",
        "name": "神崎莉央"
      },
      "text": "機嫌が直ったかどうかは、分からないわ",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_rio_12_03/sud_vo_adv_card_rio_12_03-koh009.m4a"
    },
    {
      "id": "msg_49",
      "speaker": {
        "isPlayer": false,
        "characterId": "rio",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-rio.png",
        "name": "神崎莉央"
      },
      "text": "顔を背けられちゃったから。でも効果はあったみたいね",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_rio_12_03/sud_vo_adv_card_rio_12_03-rio015.m4a"
    },
