      "id": "msg_60",
      "speaker": {
        "isPlayer": false,
        "characterId": "system",
        "icon": null,
        "name": "System"
      },
      "text": "――『LizNoir・BIG4までの軌跡』放送後",
      "isChoice": false,
      "stamp": null,
