      },
      "text": "ガイドから話を聞いてきたんだが",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_rei_10_02/sud_vo_adv_card_rei_10_02-koh017.m4a"
