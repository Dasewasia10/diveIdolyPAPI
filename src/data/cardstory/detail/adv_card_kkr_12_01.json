      "speaker": {
        "isPlayer": false,
        "characterId": "kkr",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-kokoro.png",
        "name": "赤崎こころ"
      },
      "text": "判断基準がプリンなのはおかしくないですかー！？",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_kkr_12_01/sud_vo_adv_card_kkr_12_01-kkr003.m4a"
    },
