      "id": "msg_34",
      "speaker": {
        "isPlayer": false,
        "characterId": "skr",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-sakura.png",
        "name": "川咲さくら"
      },
      "text": "おみくじを買ったり絵馬を描いたりしたんです",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_skr_03_02/sud_vo_adv_card_skr_03_02-skr010.m4a"
    },
