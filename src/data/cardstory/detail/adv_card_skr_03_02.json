      "speaker": {
        "isPlayer": false,
        "characterId": "skr",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-sakura.png",
        "name": "川咲さくら"
      },
      "text": "まさか電車が止まるとは思わなくて……",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_skr_03_02/sud_vo_adv_card_skr_03_02-koh005.m4a"
    },
