      },
      "text": "「ご飯はどこで食べよう」「お土産はどこで買おうか」",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_ktn_08_01/sud_vo_adv_card_ktn_08_01-ktn005.m4a"
