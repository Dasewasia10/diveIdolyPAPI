      "id": "msg_32",
      "speaker": {
        "isPlayer": false,
        "characterId": "kor",
        "icon": null,
        "name": "fran"
      },
      "text": "あいつがいきなりキレて、飛び出してっただけですよ",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_kor_09_01/sud_vo_adv_card_kor_09_01-kor011.m4a"
