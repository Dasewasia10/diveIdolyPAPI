      "id": "msg_11",
      "speaker": {
        "isPlayer": false,
        "characterId": "kor",
        "icon": null,
        "name": "fran"
      },
      "text": "やっぱり天才は世界に見つかってしまうんですね",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_kor_09_01/sud_vo_adv_card_kor_09_01-kor005.m4a"
    },
