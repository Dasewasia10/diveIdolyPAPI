      "speaker": {
        "isPlayer": false,
        "characterId": "kor",
        "icon": null,
        "name": "fran"
      },
      "text": "私達のアメリカ興行のレビューが載ってたんです",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_kor_09_01/sud_vo_adv_card_kor_09_01-kor003.m4a"
    },
