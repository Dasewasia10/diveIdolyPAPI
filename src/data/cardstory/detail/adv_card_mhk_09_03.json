      "speaker": {
        "isPlayer": false,
        "characterId": "mhk",
        "icon": null,
        "name": "miho"
      },
      "text": "昔のまま歌うことは、もう二度と出来ません",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mhk_09_03/sud_vo_adv_card_mhk_09_03-mhk015.m4a"
    },
