      "speaker": {
        "isPlayer": true,
        "characterId": "koh",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-makino.png",
        "name": "{user}"
      },
      "text": "……じゃなくて、さすがmihoさん",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mhk_09_03/sud_vo_adv_card_mhk_09_03-koh008.m4a"
    },
    {
      "id": "msg_33",
      "speaker": {
        "isPlayer": false,
        "characterId": "mhk",
        "icon": null,
        "name": "miho"
      },
      "text": "要するに……昔の曲も、今から私達が歌う曲も",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mhk_09_03/sud_vo_adv_card_mhk_09_03-koh008.m4a"
    },
