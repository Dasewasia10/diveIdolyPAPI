      "id": "msg_30",
      "speaker": {
        "isPlayer": false,
        "characterId": "rui",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-rui.png",
        "name": "天動瑠依"
      },
      "text": "なおさら学校生活が忙しくなるわよ",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_yu_13_01/sud_vo_adv_card_yu_13_01-rui007.m4a"
    },
