      "speaker": {
        "isPlayer": false,
        "characterId": "smr",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-sumire.png",
        "name": "奥山すみれ"
      },
      "text": "でもあの二人、すっかり打ち解けてるね",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_chn_01_01/sud_vo_adv_card_chn_01_01-chn002.m4a"
    },
