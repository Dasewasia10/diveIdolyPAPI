      "speaker": {
        "isPlayer": false,
        "characterId": "smr",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-sumire.png",
        "name": "奥山すみれ"
      },
      "text": "千紗さんもココアさんが相手だと話しやすいみたい",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_chn_01_01/sud_vo_adv_card_chn_01_01-smr002.m4a"
