      "id": "msg_36",
      "speaker": {
        "isPlayer": false,
        "characterId": "mhk",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-miho.png",
        "name": "miho"
      },
      "text": "出る時間だから、行くわ。あぁ……それとリーダーなら",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_mhk_53_02/sud_vo_adv_card_mhk_53_02-mhk011.m4a"
    },
