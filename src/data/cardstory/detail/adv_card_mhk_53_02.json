      "id": "msg_35",
      "speaker": {
        "isPlayer": false,
        "characterId": "mhk",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-miho.png",
        "name": "miho"
      },
      "text": "ⅢXとして。どんな場所でも、どんな形でもね",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_mhk_53_02/sud_vo_adv_card_mhk_53_02-mhk010.m4a"
    },
