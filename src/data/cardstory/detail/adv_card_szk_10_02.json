      "speaker": {
        "isPlayer": true,
        "characterId": "koh",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-makino.png",
        "name": "{user}"
      },
      "text": "言えなかったと思う。それからどうしたんだ？",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_szk_10_02/sud_vo_adv_card_szk_10_02-koh004.m4a"
