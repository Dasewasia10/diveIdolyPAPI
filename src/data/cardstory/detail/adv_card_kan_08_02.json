      "id": "msg_5",
      "speaker": {
        "isPlayer": false,
        "characterId": "kan",
        "icon": null,
        "name": "kana"
      },
      "text": "……あ、ちょっと待ってお父さん！",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_kan_08_02/sud_vo_adv_card_kan_08_02-kan002.m4a"
