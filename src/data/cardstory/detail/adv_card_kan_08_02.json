      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_kan_08_02/sud_vo_adv_card_kan_08_02-kan008.m4a"
    },
    {
      "id": "msg_28",
      "speaker": {
        "isPlayer": false,
        "characterId": "kan",
        "icon": null,
        "name": "kana"
      },
      "text": "正直、普通の家族ってよく分かんないけど",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_kan_08_02/sud_vo_adv_card_kan_08_02-kan009.m4a"
    },
