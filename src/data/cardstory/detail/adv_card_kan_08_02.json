      "speaker": {
        "isPlayer": true,
        "characterId": "koh",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-makino.png",
        "name": "{user}"
      },
      "text": "今回が初めてですが、何かきっかけのようなものが？",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_kan_08_02/sud_vo_adv_card_kan_08_02-koh006.m4a"
    },
    {
      "id": "msg_31",
      "speaker": {
        "isPlayer": false,
        "characterId": "kan",
        "icon": null,
        "name": "kana"
      },
      "text": "それは……アメリカに来て、お父さんに",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_kan_08_02/sud_vo_adv_card_kan_08_02-koh006.m4a"
    },
