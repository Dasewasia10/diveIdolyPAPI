      "id": "msg_32",
      "speaker": {
        "isPlayer": false,
        "characterId": "kan",
        "icon": null,
        "name": "kana"
      },
      "text": "生でステージを観てもらえたから、でしょうか",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_kan_08_02/sud_vo_adv_card_kan_08_02-kan010.m4a"
    },
