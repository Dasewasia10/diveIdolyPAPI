      "id": "msg_65",
      "speaker": {
        "isPlayer": false,
        "characterId": "hrk",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-haruko.png",
        "name": "佐伯遙子"
      },
      "text": "少し緊張してるけど……大丈夫よ",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_hrk_06_03/sud_vo_adv_card_hrk_06_03-hrk023.m4a"
    },
    {
      "id": "msg_66",
      "speaker": {
        "isPlayer": true,
        "characterId": "koh",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-makino.png",
        "name": "{user}"
      },
      "text": "（表情が硬いな……）",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_hrk_06_03/sud_vo_adv_card_hrk_06_03-koh017.m4a"
    },
