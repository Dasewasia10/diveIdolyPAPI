    {
      "id": "msg_41",
      "speaker": {
        "isPlayer": false,
        "characterId": "hrk",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-haruko.png",
        "name": "佐伯遙子"
      },
      "text": "はーい、恋人ごっこはここまで！",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_hrk_06_03/sud_vo_adv_card_hrk_06_03-hrk015.m4a"
    },
    {
      "id": "msg_42",
      "speaker": {
        "isPlayer": false,
        "characterId": "hrk",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-haruko.png",
        "name": "佐伯遙子"
      },
      "text": "やっぱり呼び捨てはやめにしましょう！",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_hrk_06_03/sud_vo_adv_card_hrk_06_03-hrk015.m4a"
