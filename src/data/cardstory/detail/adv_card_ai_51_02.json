      "speaker": {
        "isPlayer": false,
        "characterId": "ai",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-ai.png",
        "name": "小美山愛"
      },
      "text": "えっと、まずは野菜と……あれ、お父さんから電話だ",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_ai_51_02/sud_vo_adv_card_ai_51_02-ai001.m4a"
    },
    {
