      "speaker": {
        "isPlayer": false,
        "characterId": "mna",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-mana.png",
        "name": "長瀬麻奈"
      },
      "text": "……ダメかな？",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mna_09_02/sud_vo_adv_group_mna_01_14-mna009.m4a"
