      "speaker": {
        "isPlayer": true,
        "characterId": "koh",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-makino.png",
        "name": "{user}"
      },
      "text": "……曲は？　振り付けは？　どうするつもりだ？",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mna_09_02/sud_vo_adv_card_mna_09_02-koh008.m4a"
    },
    {
      "id": "msg_35",
      "speaker": {
        "isPlayer": false,
        "characterId": "mna",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-mana.png",
        "name": "長瀬麻奈"
      },
      "text": "そこは敏腕マネージャーのパワーでお願いします",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mna_09_02/sud_vo_adv_card_mna_09_02-koh008.m4a"
    },
