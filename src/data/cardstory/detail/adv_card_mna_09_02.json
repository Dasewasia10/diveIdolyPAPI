      "id": "msg_31",
      "speaker": {
        "isPlayer": false,
        "characterId": "mna",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-mana.png",
        "name": "長瀬麻奈"
      },
      "text": "見せないよ！　絶対見せないよ！",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mna_09_02/sud_vo_adv_card_mna_09_02-koh007.m4a"
    },
