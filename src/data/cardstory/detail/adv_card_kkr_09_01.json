      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_kkr_09_01/sud_vo_adv_card_kkr_09_01-kkr009.m4a"
