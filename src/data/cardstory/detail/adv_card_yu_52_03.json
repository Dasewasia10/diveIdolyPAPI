      "speaker": {
        "isPlayer": false,
        "characterId": "yu",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-yu.png",
        "name": "鈴村優"
      },
      "text": "決まってます。これから旅館に戻って新刊の準備です",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_yu_52_03/sud_vo_adv_card_yu_52_03-koh003.m4a"
