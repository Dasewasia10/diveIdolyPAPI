      "id": "msg_17",
      "speaker": {
        "isPlayer": true,
        "characterId": "koh",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-makino.png",
        "name": "{user}"
      },
      "text": "あ…はは……同人誌もいいけど、そろそろご飯の時間だ",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_yu_52_03/sud_vo_adv_card_yu_52_03-yu007.m4a"
    },
