      "speaker": {
        "isPlayer": true,
        "characterId": "koh",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-makino.png",
        "name": "{user}"
      },
      "text": "誘う相手なんていませんよ",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_rio_03_02/sud_vo_adv_card_rio_03_02-koh004.m4a"
    },
