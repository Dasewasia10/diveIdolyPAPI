      "speaker": {
        "isPlayer": false,
        "characterId": "rio",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-rio.png",
        "name": "神崎莉央"
      },
      "text": "ええ、もちろん。今度のライブバトル、必ず勝つわ",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_rio_03_02/sud_vo_adv_card_rio_03_02-rio028.m4a"
    },
