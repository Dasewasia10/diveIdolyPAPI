      "id": "msg_36",
      "speaker": {
        "isPlayer": false,
        "characterId": "rui",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-rui.png",
        "name": "天動瑠依"
      },
      "text": "いくらでも粘って、頑張って……",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_rui_11_03/sud_vo_adv_card_rui_11_03-rui012.m4a"
    },
