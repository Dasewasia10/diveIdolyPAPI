      "speaker": {
        "isPlayer": false,
        "characterId": "rei",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-rei.png",
        "name": "一ノ瀬怜"
      },
      "text": "ま、まぁ、出してはいたけど……それだけ、ほら……",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_chs_53_03/sud_vo_adv_card_chs_53_03-mei005.m4a"
    },
    {
