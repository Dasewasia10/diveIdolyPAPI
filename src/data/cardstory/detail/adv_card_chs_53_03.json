      "speaker": {
        "isPlayer": false,
        "characterId": "mei",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-mei.png",
        "name": "早坂芽衣"
      },
      "text": "って声出してたよ？　我慢しなくていいってば～",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_chs_53_03/sud_vo_adv_card_chs_53_03-mei005.m4a"
    },
    {
