      "speaker": {
        "isPlayer": false,
        "characterId": "chs",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-chisa.png",
        "name": "白石千紗"
      },
      "text": "（お姉ちゃんだけじゃなくて、サニピのみんなだけ）",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_chs_53_03/sud_vo_adv_card_chs_53_03-chs011.m4a"
