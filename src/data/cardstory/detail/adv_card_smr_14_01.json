      "speaker": {
        "isPlayer": false,
        "characterId": "smr",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-sumire.png",
        "name": "奥山すみれ"
      },
      "text": "えっと……個人的なことで申し訳ないんですけど……",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_smr_14_01/sud_vo_adv_card_smr_14_01-smr005.m4a"
    },
