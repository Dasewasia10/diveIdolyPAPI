      "speaker": {
        "isPlayer": false,
        "characterId": "unknown",
        "icon": null,
        "name": "鈴村久美子"
      },
      "text": "母親と娘やない。プロ同士やからね。分かってる？",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_yu_10_02/sud_vo_adv_card_yu_10_02-yumam001.m4a"
