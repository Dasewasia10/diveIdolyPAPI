      "speaker": {
        "isPlayer": false,
        "characterId": "unknown",
        "icon": null,
        "name": "鈴村久美子"
      },
      "text": "本番までもう少し練習したら、ええ感じになるはずや",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_yu_10_02/sud_vo_adv_card_yu_10_02-yumam002.m4a"
