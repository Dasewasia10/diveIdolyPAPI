      "speaker": {
        "isPlayer": false,
        "characterId": "yu",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-yu.png",
        "name": "鈴村優"
      },
      "text": "台本、いーっぱい読んで練習してきたんやで",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_yu_10_02/sud_vo_adv_card_yu_10_02-yu003.m4a"
    },
    {
