      "speaker": {
        "isPlayer": false,
        "characterId": "unknown",
        "icon": null,
        "name": "鈴村久美子"
      },
      "text": "うんうん、さっきの優、よく出来とったなぁ",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_yu_10_02/sud_vo_adv_card_yu_10_02-yu004.m4a"
