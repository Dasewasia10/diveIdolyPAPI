      "speaker": {
        "isPlayer": false,
        "characterId": "mna",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-mana.png",
        "name": "長瀬麻奈"
      },
      "text": "そこは――まあ、その内に……ね？",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mna_01_03/sud_vo_adv_card_mna_01_03-mna006.m4a"
    },
    {
