      "speaker": {
        "isPlayer": false,
        "characterId": "ai",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-ai.png",
        "name": "小美山愛"
      },
      "text": "パンケーキを食べながらいろんな話をしたんですけど",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_ai_02_01/sud_vo_adv_card_ai_02_01-ai014.m4a"
