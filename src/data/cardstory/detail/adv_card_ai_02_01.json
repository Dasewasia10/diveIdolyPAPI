      "speaker": {
        "isPlayer": false,
        "characterId": "ai",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-ai.png",
        "name": "小美山愛"
      },
      "text": "みんなはもう練習を終えて帰りました",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_ai_02_01/sud_vo_adv_card_ai_02_01-ai010.m4a"
