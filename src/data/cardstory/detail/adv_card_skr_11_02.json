      "speaker": {
        "isPlayer": false,
        "characterId": "skr",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-sakura.png",
        "name": "川咲さくら"
      },
      "text": "好きな食べ物を想像しちゃいました",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_skr_11_02/sud_vo_adv_card_skr_11_02-skr004.m4a"
    },
