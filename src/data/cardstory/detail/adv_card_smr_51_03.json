      "speaker": {
        "isPlayer": false,
        "characterId": "szk",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-shizuku.png",
        "name": "兵藤雫"
      },
      "text": "一生懸命で……対策、凄く頑張ってて……愛を感じた",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_smr_51_03/sud_vo_adv_card_smr_51_03-szk003.m4a"
