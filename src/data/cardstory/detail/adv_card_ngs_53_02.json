      "speaker": {
        "isPlayer": false,
        "characterId": "ngs",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-nagisa.png",
        "name": "伊吹渚"
      },
      "text": "そうだ、週末一緒に寮で観ようよ。琴乃ちゃんも誘って",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_ngs_53_02/sud_vo_adv_card_ngs_53_02-ngs010.m4a"
