      "speaker": {
        "isPlayer": false,
        "characterId": "chs",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-chisa.png",
        "name": "白石千紗"
      },
      "text": "だってあの彼氏、鼻からいきなり飛び出すんだもん",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_ngs_53_02/sud_vo_adv_card_ngs_53_02-chs004.m4a"
