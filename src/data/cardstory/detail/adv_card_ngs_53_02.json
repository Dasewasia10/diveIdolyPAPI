      "speaker": {
        "isPlayer": false,
        "characterId": "ngs",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-nagisa.png",
        "name": "伊吹渚"
      },
      "text": "うん、でも嬉しいな。あっちもこっちも分かる人がいて",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/sud_vo_adv_card_ngs_53_02/sud_vo_adv_card_ngs_53_02-chs006.m4a"
    },
