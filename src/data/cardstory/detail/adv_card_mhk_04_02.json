      "id": "msg_15",
      "speaker": {
        "isPlayer": false,
        "characterId": "mhk",
        "icon": null,
        "name": "miho"
      },
      "text": "あと、調べておきたいのは……",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mhk_04_02/sud_vo_adv_card_mhk_04_02-mhk005.m4a"
    },
