      "id": "msg_16",
      "speaker": {
        "isPlayer": false,
        "characterId": "mhk",
        "icon": null,
        "name": "miho"
      },
      "text": "……ふう。事前に確認したいのは",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mhk_04_02/sud_vo_adv_card_mhk_04_02-mhk007.m4a"
    },
