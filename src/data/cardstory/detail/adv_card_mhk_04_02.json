      "id": "msg_22",
      "speaker": {
        "isPlayer": false,
        "characterId": "mhk",
        "icon": null,
        "name": "miho"
      },
      "text": "さっき、水着は持って来ていないと言ったでしょう",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mhk_04_02/sud_vo_adv_card_mhk_04_02-koh004.m4a"
