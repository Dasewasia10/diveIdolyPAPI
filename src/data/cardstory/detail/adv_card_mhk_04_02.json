      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mhk_04_02/sud_vo_adv_card_mhk_04_02-koh007.m4a"
    },
    {
      "id": "msg_35",
      "speaker": {
        "isPlayer": false,
        "characterId": "mhk",
        "icon": null,
        "name": "miho"
      },
      "text": "なんで、まだ{user}さんがいるんです",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mhk_04_02/sud_vo_adv_card_mhk_04_02-mhk013.m4a"
    },
