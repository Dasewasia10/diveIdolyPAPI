      "speaker": {
        "isPlayer": false,
        "characterId": "kan",
        "icon": null,
        "name": "kana"
      },
      "text": "一位「♯赤崎こころ誕生祭」……？　マジでムカつく！",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_kkr_53_03/sud_vo_adv_card_kkr_53_03-kan002.m4a"
