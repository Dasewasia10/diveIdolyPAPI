        "isPlayer": false,
        "characterId": "hrk",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-haruko.png",
        "name": "佐伯遙子"
      },
      "text": "すっかり元気になっちゃった",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_hrk_07_03/sud_vo_adv_card_hrk_07_03-hrk014.m4a"
