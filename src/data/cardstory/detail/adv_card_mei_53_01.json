      "speaker": {
        "isPlayer": false,
        "characterId": "rui",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-rui.png",
        "name": "天動瑠依"
      },
      "text": "この荷物を郵便局に届けて来るわ",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mei_53_01/sud_vo_adv_card_mei_53_01-rui005.m4a"
    },
