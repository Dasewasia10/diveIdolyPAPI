      "speaker": {
        "isPlayer": false,
        "characterId": "mei",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-mei.png",
        "name": "早坂芽衣"
      },
      "text": "待ちきれないから郵便局まで取りに行こうかな～って",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mei_53_01/sud_vo_adv_card_mei_53_01-mei005.m4a"
    },
