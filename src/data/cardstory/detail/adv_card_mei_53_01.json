      "speaker": {
        "isPlayer": false,
        "characterId": "mei",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-mei.png",
        "name": "早坂芽衣"
      },
      "text": "待って！　芽衣も行く！　プレゼントが届くのを",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mei_53_01/sud_vo_adv_card_mei_53_01-rui005.m4a"
    },
