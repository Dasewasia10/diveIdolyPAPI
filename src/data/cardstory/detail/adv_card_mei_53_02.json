      "id": "msg_31",
      "speaker": {
        "isPlayer": false,
        "characterId": "mhk",
        "icon": null,
        "name": "miho"
      },
      "text": "ありがとう。あむっ……美味しい……",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mei_53_02/sud_vo_adv_card_mei_53_02-mhk006.m4a"
    },
