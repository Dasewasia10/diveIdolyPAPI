      "speaker": {
        "isPlayer": false,
        "characterId": "mhk",
        "icon": null,
        "name": "miho"
      },
      "text": "くっ……それじゃあお言葉に甘えていただこうかしら",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mei_53_02/sud_vo_adv_card_mei_53_02-mhk006.m4a"
    },
