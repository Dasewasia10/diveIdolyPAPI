      "speaker": {
        "isPlayer": false,
        "characterId": "mei",
        "icon": "https://apiip.dasewasia.my.id/iconCharacter/chara-mei.png",
        "name": "早坂芽衣"
      },
      "text": "こちらこそ一緒に食べてくれてありがとうだよ！",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_mei_53_02/sud_vo_adv_card_mei_53_02-mei010.m4a"
    },
