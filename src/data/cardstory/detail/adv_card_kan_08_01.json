      "speaker": {
        "isPlayer": false,
        "characterId": "kan",
        "icon": null,
        "name": "kana"
      },
      "text": "kana、昨日のフェスでまだ疲れてるんだけど",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_kan_08_01/sud_vo_adv_card_kan_08_01-kan001.m4a"
    },
    {
