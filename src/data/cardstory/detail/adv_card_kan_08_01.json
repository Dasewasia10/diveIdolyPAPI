      "id": "msg_16",
      "speaker": {
        "isPlayer": false,
        "characterId": "kan",
        "icon": null,
        "name": "kana"
      },
      "text": "……『ⅢXのkana、知られざる悲劇の生い立ち",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_kan_08_01/sud_vo_adv_card_kan_08_01-kan004.m4a"
    },
