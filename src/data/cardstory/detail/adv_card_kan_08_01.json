      "id": "msg_19",
      "speaker": {
        "isPlayer": false,
        "characterId": "kan",
        "icon": null,
        "name": "kana"
      },
      "text": "一般人恫喝して、スクープ気取るなっての！！",
      "isChoice": false,
      "stamp": null,
      "image": null,
      "voiceUrl": "https://apiip.dasewasia.my.id/cardStoryVoice/adv_card_kan_08_01/sud_vo_adv_card_kan_08_01-kan006.m4a"
