      "text": "詳しいわね",
      "startTime": 167.2333333333,
      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "mhk",
      "speakerName": "miho",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-miho.png",
