    {
      "type": "dialogue",
      "speakerCode": null,
      "speakerName": "",
      "iconUrl": null,
      "voiceUrl": null,
      "text": "――五年前",
      "startTime": 20.2000001907,
      "sfxList": [
        {
          "src": "https://api.diveidolypapi.my.id/storyBgm/sud_se_adv_walk-out-single-01.m4a",
