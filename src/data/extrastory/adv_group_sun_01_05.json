    },
    {
      "type": "background",
      "src": "https://api.diveidolypapi.my.id/storyBackground/env_adv_2d_shiraliving-00-noon.webp",
      "bgName": "shiraliving-00-noon"
    },
    {
      "type": "dialogue",
      "speakerCode": "koh",
      "speakerName": "{user}",
