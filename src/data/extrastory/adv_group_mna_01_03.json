    },
    {
      "type": "dialogue",
      "speakerCode": "shj",
      "speakerName": "三枝信司",
      "iconUrl": null,
      "voiceUrl": "https://api.diveidolypapi.my.id/extraStoryVoice/sud_vo_adv_group_mna_01_03/sud_vo_adv_group_mna_01_03-shj008.m4a",
      "text": "俺ももちろんフォローはするさ\nまあ、どのみち、男手は欲しかったからな",
      "startTime": 89.13333333333334,
      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "hrk",
      "speakerName": "佐伯遙子",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-haruko.png",
      "voiceUrl": "https://api.diveidolypapi.my.id/extraStoryVoice/sud_vo_adv_group_mna_01_03/sud_vo_adv_group_mna_01_03-hrk004.m4a",
      "text": "はあ",
      "startTime": 95.63333333333334,
      "sfxList": []
    },
    {
      "type": "bgm",
      "action": "play",
      "src": "https://api.diveidolypapi.my.id/storyBgm/sud_bgm_adv_lesson-01.m4a"
    },
    {
      "type": "dialogue",
