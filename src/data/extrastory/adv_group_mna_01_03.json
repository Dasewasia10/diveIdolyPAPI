      "startTime": 163.93333333333334,
      "sfxList": []
    },
    {
      "type": "background",
      "src": "https://api.diveidolypapi.my.id/storyBackground/env_adv_2d_train-00-noon.webp",
      "bgName": "train-00-noon"
    },
    {
