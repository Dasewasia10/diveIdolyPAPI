      "voiceUrl": "https://api.diveidolypapi.my.id/extraStoryVoice/sud_vo_adv_group_mna_01_03/sud_vo_adv_group_mna_01_03-mna001.m4a",
      "text": "はい！",
      "startTime": 10.8,
      "sfxList": []
    },
    {
      "type": "background",
      "src": "https://api.diveidolypapi.my.id/storyBackground/env_adv_2d_hoshireception-00-noon.webp",
