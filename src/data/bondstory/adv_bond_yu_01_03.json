    {
      "type": "dialogue",
      "speakerCode": "rui",
      "speakerName": "天動瑠依",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-rui.png",
      "voiceUrl": null,
      "text": "この辺で撮影があったの。ちょうどさっき終わったのよ",
      "startTime": 29.4333354314,
      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "rui",
