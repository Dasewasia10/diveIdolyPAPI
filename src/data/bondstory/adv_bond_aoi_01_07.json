        }
      ]
    },
    {
      "type": "dialogue",
      "speakerCode": "skr",
      "speakerName": "川咲さくら",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-sakura.png",
      "voiceUrl": "https://api.diveidolypapi.my.id/bondStoryVoice/sud_vo_adv_bond_aoi_01_07/sud_vo_adv_bond_aoi_01_07-skr005.m4a",
      "text": "くくくっ、いつ見ても『ホットドッグさん』は\n面白いな～動画一気見しちゃったよ",
      "startTime": 55.9999966939,
      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "skr",
      "speakerName": "川咲さくら",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-sakura.png",
