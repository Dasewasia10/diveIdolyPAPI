      "speakerCode": null,
      "speakerName": "",
      "iconUrl": null,
      "voiceUrl": null,
      "text": "――ファッション業界のパーティー",
      "startTime": 0,
      "sfxList": []
    },
    {
      "type": "background",
      "src": "https://api.diveidolypapi.my.id/storyBackground/env_adv_2d_preentrance-00-noon.webp",
