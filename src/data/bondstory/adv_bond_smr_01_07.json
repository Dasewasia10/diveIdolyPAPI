      "bgName": "s2"
    },
    {
      "type": "dialogue",
      "speakerCode": "smr",
      "speakerName": "奥山すみれ",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-sumire.png",
      "voiceUrl": "https://api.diveidolypapi.my.id/bondStoryVoice/sud_vo_adv_bond_smr_01_07/sud_vo_adv_bond_smr_01_07-smr003.m4a",
      "text": "でも学校までまだまだ遠いし……",
      "startTime": 30.2000003179,
      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "smr",
      "speakerName": "奥山すみれ",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-sumire.png",
      "voiceUrl": null,
      "text": "ちょっとだけお弁当、つまみ食いしちゃおっと！",
      "startTime": 34.5000012081,
      "sfxList": []
    },
    {
      "type": "background",
      "src": "https://api.diveidolypapi.my.id/storyBackground/env_adv_2d_mountainroad-00-noon.webp",
      "bgName": "s2"
    },
    {
      "type": "dialogue",
      "speakerCode": "smr",
      "speakerName": "奥山すみれ",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-sumire.png",
