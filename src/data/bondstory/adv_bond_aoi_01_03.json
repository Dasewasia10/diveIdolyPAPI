    {
      "type": "dialogue",
      "speakerCode": "aoi",
      "speakerName": "井川葵",
      "iconUrl": "https://apiip.dasewasia.my.id/iconCharacter/chara-aoi.png",
      "voiceUrl": "https://apiip.dasewasia.my.id/bondStoryVoice/sud_vo_adv_bond_aoi_01_03/sud_vo_adv_bond_aoi_01_03-aoi007.m4a",
      "text": "ごめん、愛の飲み込みが早いから嬉しくて\nつい……やり過ぎたよ。次は愛の好きなゲームをしよう",
      "startTime": 67.6000033061,
      "sfxList": []
    },
    {
      "type": "background",
      "src": "https://apiip.dasewasia.my.id/storyBackground/env_adv_2d_gamecenter-00-noon.webp",
      "bgName": "gamecenter-00-noon"
    },
    {
      "type": "dialogue",
      "speakerCode": "ai",
      "speakerName": "小美山愛",
      "iconUrl": "https://apiip.dasewasia.my.id/iconCharacter/chara-ai.png",
