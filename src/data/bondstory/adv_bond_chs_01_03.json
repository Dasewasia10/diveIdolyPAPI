    {
      "type": "dialogue",
      "speakerCode": "szk",
      "speakerName": "兵藤雫",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-shizuku.png",
      "voiceUrl": "https://api.diveidolypapi.my.id/bondStoryVoice/sud_vo_adv_bond_chs_01_03/sud_vo_adv_bond_chs_01_03-szk007.m4a",
      "text": "私しか聴いてないから、大丈夫\n間違えちゃっても、誰も怒らない",
      "startTime": 88.1999989827,
      "sfxList": []
    },
    {
      "type": "background",
