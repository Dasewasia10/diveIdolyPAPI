    },
    {
      "type": "dialogue",
      "speakerCode": "ai",
      "speakerName": "小美山愛",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-ai.png",
      "voiceUrl": null,
      "text": "え、えぇっ！？",
