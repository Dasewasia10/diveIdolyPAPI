    },
    {
      "type": "dialogue",
      "speakerCode": "aoi",
      "speakerName": "井川葵",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-aoi.png",
      "voiceUrl": "https://api.diveidolypapi.my.id/bondStoryVoice/sud_vo_adv_bond_aoi_01_01/sud_vo_adv_bond_aoi_01_01-aoi007.m4a",
