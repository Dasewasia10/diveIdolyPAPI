      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "ai",
      "speakerName": "小美山愛",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-ai.png",
      "voiceUrl": "https://api.diveidolypapi.my.id/bondStoryVoice/sud_vo_adv_bond_aoi_01_01/sud_vo_adv_bond_aoi_01_01-ai005.m4a",
      "text": "葵さんと、今からここでですか！？",
