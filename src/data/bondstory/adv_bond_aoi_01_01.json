    },
    {
      "type": "dialogue",
      "speakerCode": "ai",
      "speakerName": "小美山愛",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-ai.png",
      "voiceUrl": null,
      "text": "えぇ！？　あの短い間奏中に二回転ですか？",
      "startTime": 35.7666674297,
      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "ai",
      "speakerName": "小美山愛",
