    {
      "type": "background",
      "src": "https://api.diveidolypapi.my.id/storyBackground/env_adv_2d_livestage-00-noon.webp",
      "bgName": "livestage-00-noon"
    },
    {
      "type": "dialogue",
      "speakerCode": "hrk",
