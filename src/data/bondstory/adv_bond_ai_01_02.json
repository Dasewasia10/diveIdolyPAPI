      "startTime": 57.6333333333,
      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "ai",
      "speakerName": "小美山愛",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-ai.png",
