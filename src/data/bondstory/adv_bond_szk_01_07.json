    {
      "type": "dialogue",
      "speakerCode": "ai",
      "speakerName": "小美山愛",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-ai.png",
      "voiceUrl": null,
      "text": "うう……お腹空いちゃった……夜中にカップラーメン",
      "startTime": 103.0000019433,
      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "ai",
      "speakerName": "小美山愛",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-ai.png",
      "voiceUrl": "https://api.diveidolypapi.my.id/bondStoryVoice/sud_vo_adv_bond_szk_01_07/sud_vo_adv_bond_szk_01_07-ai007.m4a",
      "text": "なんて莉央さんに怒られちゃいそうだけど……",
      "startTime": 108.6333330791,
      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "ai",
      "speakerName": "小美山愛",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-ai.png",
      "voiceUrl": null,
      "text": "こんな時間に玄関から物音……！？　もしかして……",
      "startTime": 112.7666803083,
      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "ai",
      "speakerName": "小美山愛",
