    {
      "type": "background",
      "src": "https://apiip.dasewasia.my.id/storyBackground/env_adv_2d_tokyoyuroom-00-noon.webp",
      "bgName": "tokyoyuroom-00-noon"
    },
    {
      "type": "dialogue",
      "speakerCode": "yu",
      "speakerName": "鈴村優",
      "iconUrl": "https://apiip.dasewasia.my.id/iconCharacter/chara-yu.png",
      "voiceUrl": "https://apiip.dasewasia.my.id/bondStoryVoice/sud_vo_adv_bond_yu_01_02/sud_vo_adv_bond_yu_01_02-yu011.m4a",
      "text": "瑠依ちゃん……そう言われるのは嬉しいねんけど\nあかんねん……！　うち、今から画塾行ってくるわ！",
      "startTime": 136.7999947229,
      "sfxList": []
    },
    {
      "type": "background",
      "src": "https://apiip.dasewasia.my.id/storyBackground/env_adv_2d_tokyoyuroom-00-noon.webp",
      "bgName": "tokyoyuroom-00-noon"
    },
    {
      "type": "dialogue",
      "speakerCode": "rui",
      "speakerName": "天動瑠依",
      "iconUrl": "https://apiip.dasewasia.my.id/iconCharacter/chara-rui.png",
      "voiceUrl": "https://apiip.dasewasia.my.id/bondStoryVoice/sud_vo_adv_bond_yu_01_02/sud_vo_adv_bond_yu_01_02-rui009.m4a",
      "text": "ちょ、ちょっと優！？　本当に画塾に行くつもり！？\nだ、誰か優を止めてー！",
      "startTime": 146.1999855676,
