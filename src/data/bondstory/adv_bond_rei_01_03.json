      "voiceUrl": null,
      "text": "もう、ほんっとにうちのお父さんて",
      "startTime": 0.8,
      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "rei",
      "speakerName": "一ノ瀬怜",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-rei.png",
