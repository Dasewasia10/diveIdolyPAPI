      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "rio",
      "speakerName": "神崎莉央",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-rio.png",
      "voiceUrl": "https://api.diveidolypapi.my.id/bondStoryVoice/sud_vo_adv_bond_rui_01_04/sud_vo_adv_bond_rui_01_04-rio006.m4a",
      "text": "ちょっと！　不用意に驚かさないで！",
      "startTime": 47.900001526,
      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "rio",
      "speakerName": "神崎莉央",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-rio.png",
      "voiceUrl": null,
