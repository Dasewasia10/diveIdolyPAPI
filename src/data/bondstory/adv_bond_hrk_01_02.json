      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-haruko.png",
      "voiceUrl": null,
      "text": "（次はどこの廃墟に行こうかしら）",
      "startTime": 0.8,
      "sfxList": [
        {
          "src": "https://api.diveidolypapi.my.id/storyBgm/sud_se_adv_phone-tap-02.m4a",
