      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "szk",
      "speakerName": "兵藤雫",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-shizuku.png",
      "voiceUrl": "https://api.diveidolypapi.my.id/bondStoryVoice/sud_vo_adv_bond_chs_01_02/sud_vo_adv_bond_chs_01_02-szk005.m4a",
      "text": "（テーブルの上に、美味しそうなバナナとリンゴが……）",
      "startTime": 69.3,
      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "szk",
