      "voiceUrl": "https://api.diveidolypapi.my.id/bondStoryVoice/sud_vo_adv_bond_chs_01_02/sud_vo_adv_bond_chs_01_02-szk007.m4a",
      "text": "……？　…………？",
      "startTime": 82.8154626079,
      "sfxList": [
        {
          "src": "https://api.diveidolypapi.my.id/storyBgm/sud_se_adv_walk-living-single-01.m4a",
