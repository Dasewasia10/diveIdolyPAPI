      "startTime": 86.7999988556,
      "sfxList": []
    },
    {
      "type": "dialogue",
      "speakerCode": "chs",
      "speakerName": "白石千紗",
      "iconUrl": "https://api.diveidolypapi.my.id/iconCharacter/chara-chisa.png",
